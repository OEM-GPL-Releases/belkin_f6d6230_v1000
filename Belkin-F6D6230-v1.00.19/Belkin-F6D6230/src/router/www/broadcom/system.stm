function Rgmt_Apply()
{
	var f=document.forms[0];
	if(f.EnableRgmt[0].checked)
	{
		f.checkremote.disabled = false;
		f.http_wanport.disabled = false;
		EnableDisableIPTextbox();
	}
	else
	{
		f.checkremote.disabled = true;
		f.http_wanport.disabled = true;
		f.IP1.disabled = true;
		f.IP2.disabled = true;
		f.IP3.disabled = true;
		f.IP4.disabled = true;
	}
}

function EnableDisableIPTextbox()
{
	var f=document.forms[0];
 	if(f.checkremote.checked == true)
