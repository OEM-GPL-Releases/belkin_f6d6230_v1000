function storeData()
{
	var f=document.forms[0];

	if (f.daylight.checked == true)
		f.time_zone_DST.value = '1';
	else
		f.time_zone_DST.value = '0';

	if (f.EnableUPNP[0].checked==true) 
		f.upnp_enable.value =1;
 	else 
		f.upnp_enable.value =0;
		 
	
	if (f.version_eb[0].checked==true)
		f.remote_auto_fw.value =1;
	else
		f.remote_auto_fw.value =0;	
	
	f.ntp_server0.value=time_server[document.forms[0].time1.selectedIndex];;
	f.ntp_server1.value=time_server[document.forms[0].time2.selectedIndex];;
	return true;
}

function Try_Submit() 
{
	var f=document.forms[0];
	var tmp,i,c;
      var passwd="<% nvram_js_get("http_passwd"); %>";
      
      if(f.http_passwd.value=="" && f.userOldPswd_t.value=="")
 	{
 	  f.http_passwd.value=passwd; 
 	  f.userConPswd.value= f.http_passwd.value;
 	}
 	else
 	{
 	 if((f.userOldPswd_t.value!=passwd))
 	 {
 	 alert("Invalid current password!");
 	 return false;
 	 }
 	  	
 	 if((f.http_passwd.value.length<3 || f.http_passwd.value.length>12) && f.http_passwd.value!="")
 	 {
 	  alert("Administration password length should be between 3 and 12.");
 	  return false;
 	 }
 	}
 	
 	if(f.http_passwd.value!=f.userConPswd.value)
 	{
 	 alert("Password and confirm password do not match.");
 	 return false;

 	}
 	
	ns = f.login_timeout.value;

       for (i=0;i<ns.length;i++)
       {
    	c=ns.charAt(i);
        if ((c<'0')||(c>'9'))
		{
			 alert(idletimeerror);
			 return false;
		}	
	}
 	 if(eval(f.login_timeout.value)<1 || eval(f.login_timeout.value)>99)
 	{
		alert(idletimeerror);
		return false;
 	}

//	if(storeData()==false)
//		return false;
	
	if(f.EnableRgmt[0].checked)
	{
		if(f.checkremote.checked == true)
	 	{
			//f.remote_mgmt_ip.value="";
			f.allow_remote_ip.value="0";
			f.remote_mgmt_enabled.value = "1";

			if(f.http_wanport.value=="")
				f.http_wanport.value="8080";
			else 
			{
				if((f.http_wanport.value < 1 ) || (f.http_wanport.value >65535))
				{
					alert(porterror);
					f.http_wanport.focus();				
					return false;
				}
			}
		}
		else
	 	{
			if(isNValidIP(f.IP1.value)) { alert(usysipm); return false; }
			if(isNValidIP(f.IP2.value)) { alert(usysipm); return false; }
			if(isNValidIP(f.IP3.value)) { alert(usysipm); return false; }
			if(isNValidLastIP(f.IP4.value)) { alert(usysipm); return false; }
			
			f.remote_mgmt_ip.value = f.IP1.value + '.' + f.IP2.value + '.' + f.IP3.value + '.' + f.IP4.value;
			f.allow_remote_ip.value="1";
			f.remote_mgmt_enabled.value = "1";

			if(isBlank(f.http_wanport.value))
				f.http_wanport.value="8080";
			else 
			{
				if((f.http_wanport.value < 1 ) || (f.http_wanport.value > 65535)) 
				{
					alert(porterror);
					f.http_wanport.focus();
					return false;
				}
			}
	 	}
 	}
	else
	{
		f.remote_mgmt_enabled.value = "0";
		f.allow_remote_ip.value="1";
		f.remote_mgmt_ip.value = "";
		f.http_wanport.value = '8080';
	}
	
	//temp = f.curlang.options[f.curlang.options.selectedIndex].value.split("+");
	//f.lang_code.value = temp[0];
	//f.charset.value = temp[1]; //lang_code charset country_code
//	for(i=0; i<5; i++)
//	{
//		if(document.forms[0].curlang.options[i].selected == true){
//				f.lang_code.value = i;
//				break
//				}
//	}
//	for(i=0; i<4; i++)
//	{
//		if(document.forms[0].country_code.options[i].selected == true){
//				f.country_codevalue.value = i;
//				break
//				}
//	}
 

 /* time server */ 
  f.time_zone.value=time_zone_list[f.time_zone_index.selectedIndex];
   f.dst_time_zone.value=dst_time_zone_list[f.time_zone_index.selectedIndex];
 	f.ntp_server0.value=time_server[document.forms[0].time1.selectedIndex];
	f.ntp_server1.value=time_server[document.forms[0].time2.selectedIndex];
	
	if(f.daylight.checked == true) 
		f.time_zone_DST.value=1;
	else 
		f.time_zone_DST.value=0;
	
	if (f.version_eb[0].checked==true)
		f.remote_auto_fw.value =1;
	else
		f.remote_auto_fw.value =0;	
		
	UPNP_Apply();
	
	/******  ECO setting ******/
	if(f.dim_icons.checked==true)
		f.eco_led_dim.value=1;
	else
		f.eco_led_dim.value=0;
		
	if(f.dis_rad_from.checked==true)
	{
		f.eco_wlan.value=1;

            if(f.start_hour.selectedIndex>=f.end_hour.selectedIndex)
	     {
	        	alert("End time must be later than start time for disabling radio" );
	  		return false;
	      }
	}	
	else
		f.eco_wlan.value=0;		
		
	
	f.eco_wlan_start.value=f.start_hour.selectedIndex;
	f.eco_wlan_end.value=f.end_hour.selectedIndex;

  f.eco_wlan_days0.value=(f.eco_days_7.checked==true)?"1":"0";
  f.eco_wlan_days1.value=(f.eco_days_1.checked==true)?"1":"0";
  f.eco_wlan_days2.value=(f.eco_days_2.checked==true)?"1":"0";
  f.eco_wlan_days3.value=(f.eco_days_3.checked==true)?"1":"0";
  f.eco_wlan_days4.value=(f.eco_days_4.checked==true)?"1":"0";
  f.eco_wlan_days5.value=(f.eco_days_5.checked==true)?"1":"0";
  f.eco_wlan_days6.value=(f.eco_days_6.checked==true)?"1":"0";

/**************************/
	f.http_passwd.maxLength = 64;
	f.http_passwd.value = encode(f.http_passwd.value);
	f.userOldPswd_t.value = "";
 	f.userConPswd.value = "";

	return true;
}



