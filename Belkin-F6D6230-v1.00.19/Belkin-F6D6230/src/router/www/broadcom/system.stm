function frmload()
{

if(document.forms[0].remote_mgmt_ip.value!="")
{
	var spli=(document.forms[0].remote_mgmt_ip.value).split(".");
	document.forms[0].IP1.value=spli[0];
	document.forms[0].IP2.value=spli[1];
	document.forms[0].IP3.value=spli[2];
	document.forms[0].IP4.value=spli[3];
}	
//document.forms[0].http_wanport.value=gs_remote_mgmt_port;
//document.forms[0].userOldPswd_t.value=http_passwd;
//document.forms[0].timeout.value=gs_login_timeout;
var eco_enable=<% nvram_get("wl_bss_enabled"); %>;
var f=document.forms[0];

if(eco_enable==0)
{
f.dis_rad_from.disabled=true;
f.end_hour.disabled=true;
f.start_hour.disabled=true;
f.eco_days_7.disabled=f.eco_days_6.disabled=f.eco_days_5.disabled=f.eco_days_4.disabled=f.eco_days_3.disabled=f.eco_days_2.disabled=f.eco_days_1.disabled=true;
}

if(upnp_enabled ==1) 
		f.EnableUPNP[0].checked=true;
 	else 
		f.EnableUPNP[1].checked=true;
if(gs_fw_auto==1)
f.version_eb[0].checked=true;
 	else 
f.version_eb[1].checked=true;

Rgmt_Apply();

funClock();
}

