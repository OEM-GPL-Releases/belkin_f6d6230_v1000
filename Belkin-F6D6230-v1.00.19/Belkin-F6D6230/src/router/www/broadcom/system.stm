											}
											else
											{
												
												f.IP1.value=remote_ip.substring(0,t1);
												f.IP2.value=remote_ip.substring(t1+1,t2);
												f.IP3.value=remote_ip.substring(t2+1,t3);
												f.IP4.value=remote_ip.substring(t3+1);
												//f.http_wanport.value=f.http_wanport.value;
											}
											
